    <main>
        <div class="cafe-box">
            
            
            <% productos.forEach(producto => { %>
               
                     
                     <% if(producto.categoria == "cafe") {%> 
                    <div class="producto-cafe">
                     <a href=/producto/<%=producto.id %>> <img src=<%=producto.imagen %> alt="Cafe"> </a> 
                     <p><%=producto.nombre %></p>
                     
                </div>
                <% } %> 
           <% }) %> 
                
        </div>
                   




    </main>
  <!--footer start-->
